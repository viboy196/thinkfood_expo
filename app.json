{
  "expo": {
    "name": "ThinkFood",
    "slug": "thinkfood_expo",
    "version": "1.0.14",
    "orientation": "portrait",
    "icon": "./assets/images/logo/thinkfoodlogo.png",
    "scheme": "myapp",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo/logo_notification.png",
          "color": "#ffffff"
        }
      ]
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thinkfood.user",
      "buildNumber": "18"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thinkfood.user",
      "versionCode": 6,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "afe848fd-9b5f-4416-87e1-9d28e31225f6"
      }
    }
  }
}
