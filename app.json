{
  "expo": {
    "name": "thinkfood_expo",
    "slug": "thinkfood_expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/thinkfoodlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
